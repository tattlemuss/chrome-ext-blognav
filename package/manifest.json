{
  "manifest_version": 2,

  "name": "Easy blog navigation",
  "description": ".",
  "version": "0.1",
  
    "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["contentscript.js"]
    }
  ],
  
  "web_accessible_resources": [
    "blognav.css",
    "icon.png"
  ]
}
